{
  "name": "fiary",
  "description": "personal financial diary",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:4000/api",
  "engines": {
    "node": "10.6.0"
  },
  "scripts": {
    "start": "react-scripts build && NODE_ENV=production NODE_PATH=src/ node server/app.js",
    "build": "NODE_PATH=src/ react-scripts build",
    "deploy:test": "git push heroku master",
    "dev:front": "NODE_PATH=src/ react-scripts start",
    "dev:back": "NODE_ENV=development LOGGER=true nodemon server/app.js",
    "dev": "yarn dev:back & yarn dev:front",
    "test": "NODE_ENV=test jest server/ --detectOpenHandles --forceExit --verbose --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test:front": "NODE_PATH=src/ react-scripts test --env=jsdom --verbose",
    "test:front:cov": "NODE_PATH=src/ react-scripts test --env=jsdom --coverage",
    "test:back": "NODE_ENV=test jest server/ --watch --verbose --detectOpenHandles",
    "test:back:cov": "NODE_ENV=test jest server/ --coverage --detectOpenHandles",
    "see:cov": "xdg-open coverage/lcov-report/index.html > /dev/null 2>&1 &",
    "seq": "node_modules/.bin/knex",
    "gen:migrate": "knex migrate:make rename_me",
    "db:create": "node db/utils/create.js",
    "db:drop": "node db/utils/drop.js",
    "db:migrate": "node_modules/.bin/knex migrate:latest",
    "db:migrate:undo": "node_modules/.bin/knex migrate:rollback",
    "db:backup": "heroku pg:backups:capture && heroku pg:backups:download",
    "db:backup:prod": "pg_dump --clean fiary_prod -F c > tmp/$(date +%F).dump",
    "db:restore": "yarn db:drop && yarn db:create && pg_restore -U fiary -d fiary_dev latest.dump --verbose --clean --no-acl --no-owner",
    "deploy:test:migrate": "heroku run npm run db:migrate",
    "deploy": "git pull && yarn && NODE_ENV=production yarn db:migrate && yarn build && pm2 restart app",
    "eject": "react-scripts eject",
    "routes": "node scripts/routes.js",
    "postpublish": "PACKAGE_VERSION=$(cat package.json | grep version | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git tag $PACKAGE_VERSION && git push --tags",
    "repl": "NODE_ENV=development node --experimental-repl-await scripts/repl.js"
  },
  "dependencies": {
    "@material-ui/core": "^3.3.0",
    "@material-ui/icons": "^3.0.1",
    "axios": "^0.16.2",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.17.2",
    "bookshelf": "^0.10.4",
    "bookshelf-camelcase": "https://github.com/mandelbroo/bookshelf-camelcase",
    "bookshelf-cascade-delete": "^2.0.1",
    "bookshelf-modelbase": "^2.10.4",
    "chartist": "^0.11.0",
    "classnames": "^2.2.6",
    "compression": "^1.7.1",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-validation": "^1.0.2",
    "joi": "^13.0.0",
    "jsonwebtoken": "^8.0.0",
    "jwt-decode": "^2.2.0",
    "knex": "^0.14.3",
    "lodash": "^4.17.11",
    "luxon": "^0.3.0",
    "morgan-body": "^0.10.9",
    "pg": "^7.5.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-calendar": "^2.17.4",
    "react-dom": "^16.6.3",
    "react-jss": "^8.6.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^2.1.3",
    "react-sidebar": "^2.3.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^5.0.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "dotenv": "^4.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-plugin-jest": "^21.23.0",
    "eslint-plugin-prettier": "^2.6.2",
    "jest": "^23.6.0",
    "jest-localstorage-mock": "^2.2.0",
    "nodemon": "^1.18.3",
    "supertest": "^3.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
